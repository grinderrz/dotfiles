[user]
	name = Grigoriy Glushko
	email = gglushko@lingualeo.com
[core]
	editor = vim
[merge]
	tool = vimdiff
[color]
	ui = true
[alias]
	co = checkout
	ci = commit
	br = branch
	st = status
	untrack = update-index
	lsuntrack = ls-files
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[credential]
	helper = osxkeychain
